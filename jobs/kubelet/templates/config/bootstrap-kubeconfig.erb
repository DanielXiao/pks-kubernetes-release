apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubelet/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet-bootstrap
  name: kubelet
current-context: kubelet
users:
- name: kubelet-bootstrap
  user:
    token: <%= p('tls.bootstrap_token') %>
