apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubelet/config/apiserver-ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet-pre-stop
  name: kubelet-pre-stop
current-context: kubelet-pre-stop
users:
- name: kubelet-pre-stop
  user:
    token: "<%= p("pre-stop-api-token") %>"
