apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /var/run/kubernetes/requestheader-client-ca.crt
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: "metrics-server"
  name: context
current-context: context
users:
- name: "metrics-server"
  user:
    client-certificate: /var/run/kubernetes/client.crt
    client-key: /var/run/kubernetes/client.key
